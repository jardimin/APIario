<tr id="deliveries_<%= state_change.id %>" style="display:none;">
  <td colspan="5">
    <table class="table" width="100%">
      <tr class="odd">
    		<th>Type</th>
    		<th>Value</th>
    		<th>State</th>
    	</tr>

      <% for delivery in state_change.deliveries %>
        <tr>
      		<td><%= delivery.try(:notification).try(:name) || 'no data' %></td>
      		<td><%= delivery.try(:notification).try(:value) || 'no data' %></td>
      		<td><%= delivery.state.titleize %> after <%= notified_at delivery, @job %></td>
      	</tr>	
      <% end %>
    </table>
  </td>
</tr>
